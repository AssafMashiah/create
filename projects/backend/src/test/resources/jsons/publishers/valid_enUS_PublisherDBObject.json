  "ttsProviders": [
    {
      "id": 1,
      "name": "ReadSpeaker",
      "locales": [
        "en_us"
      ],
      "configurations": {
        "admin": [
          {
            "key": "key",
            "type": "text",
            "attributes": {
              "required": true
            },
            "validation": {
              "required": true
            },
            "is_input": true,
            "attributes_str": "required=true",
            "selected": "שדג"
          }
        ],
        "course": [
          {
            "key": "volume",
            "type": "number",
            "attributes": {
              "min": 0,
              "max": 200
            },
            "default": 100
          },
          {
            "key": "pitch",
            "type": "number",
            "attributes": {
              "min": 0,
              "max": 200
            },
            "default": 100
          },
          {
            "key": "speed",
            "type": "number",
            "attributes": {
              "min": 0,
              "max": 200
            },
            "default": 100
          },
          {
            "key": "voice",
            "type": "multiselection",
            "filterOptionsByLocale": true,
            "options": {
              "en_us": [
                "Kate",
                "Paul"
              ]
            },
            "default": {
              "key": "en_us",
              "index": 0,
              "indexValue": "Paul"
            }
          },
          {
            "key": "mp3bitrate",
            "type": "number",
            "attributes": {
              "min": 16,
              "max": 128,
              "step": 8
            },
            "default": 128
          },
          {
            "key": "dictionary",
            "type": "array",
            "options": [
              "on",
              "off"
            ],
            "default": {
              "default": 0,
              "indexValue": "on"
            }
          }
        ],
        "api": {
          "template": {
            "data": {
              "base_url": "http://tts.readspeaker.com/a/speak",
              "appId": 1,
              "command": "produce",
              "charset": "UTF-8",
              "textformat": "plain",
              "audioformat": "mp3",
              "container": "wave"
            },
            "path": "{{base_url}}?key={{key}}&command={{command}}&appId={{appId}}&charset={{charset}}&textformat={{textformat}}&text={{text}}&volume={{volume}}&pitch={{pitch}}&speed={{speed}}&audioformat={{audioformat}}&container={{container}}&samplerate={{samplerate}}&sampledepth={{sampledepth}}&mp3bitrate={{mp3bitrate}}&dictionary={{dictionary}}&lang={{lang}}&voice={{voice}}"
          },
          "method": "POST"
        }
      }
    }
  ]
