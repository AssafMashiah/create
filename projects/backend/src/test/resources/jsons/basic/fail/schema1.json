{
	"$schema": "http://json-schema.org/draft-03/schema",
	"type": ["object","string"],
	"properties": {
		"geo": {"$ref": "http://jsonschema.net/examples/C.json"},
		"address": {
			"type": "object",
			"properties": {
				"city": {
					"type": "string"
				},
				"streetAddress": {
					"type": "string"
				}
			}
		},
		"age":{
			"type": "number",
            "required":true
		}
	}
}