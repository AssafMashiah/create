<progress id="{{id}}">
	<ignoreChecking>
		{{#ignoreCheckingItems}}
			<item type="{{id}}"></item>
		{{/ignoreCheckingItems}}
	</ignoreChecking>
	{{#score}}<points>{{score}}</points>{{/score}}
	<attempts>{{num_of_attempts}}</attempts>
	{{#showProgressType}}
		<type>{{progressType}}</type>
	{{/showProgressType}}
	<checkable>{{checkable}}</checkable>
	<timer>
		<enabled>{{timer_enabled}}</enabled>
		<minutes>{{timer_minutes}}</minutes>
		<seconds>{{timer_seconds}}</seconds>
		<totalSeconds>{{timer_total_seconds}}</totalSeconds>
	</timer>
    {{{content}}}
</progress>
